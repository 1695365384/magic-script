{
  "name": "@bundy-lmw/magic_script",
  "version": "1.0.0",
  "description": "一个可插拔的企业级react微前端脚手架",
  "main": "./bin/magic",
  "bin": {
    "magic": "./bin/magic"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development magic --port=4000 start",
    "build": "rimraf ./dist && cross-env NODE_ENV=production magic build",
    "eslint": "eslint --ext .tsx,.ts --fix ./src",
    "lint": "npm run lint:js",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:js": "eslint --cache --ext .ts,.tsx ./src",
    "lint:style": "stylelint --fix \"**/*.{css,scss,sass}\" ",
    "prettier:fix": "prettier --write src/**/*.{js,jsx,ts,tsx,scss,less}",
    "analyze": "cross-env NODE_ENV=production magic analyze",
    "init": "npm link"
  },
  "keywords": [
    "magic",
    "magic-react",
    "magic-script"
  ],
  "author": "bundy-lmw",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.16.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": ">=3.0.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.4",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.9",
    "@types/react-router-config": "^5.0.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.26.1",
    "autoprefixer": "^10.3.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "cac": "^6.7.3",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^9.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^1.3.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^7.3.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^6.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "1.19.1",
    "pretty-quick": "^3.1.0",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "9.1.1",
    "typed-css-modules-loader": "0.0.18",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3",
    "webpack-parallel-uglify-plugin": "^2.0.0",
    "webpackbar": "^5.0.0-3",
    "yargs": "17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{jsx,tsx,ts,js,json,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}